def is_prime(n):
    if n < 2:
        return False
    if n == 2:
        return True
    for i in range(2,int(n**0.5)+1):
        if (n % i) ==0:
            return False
    else: 
        return True

for m in range(80,120):
    if is_prime(m):
        print(m)











